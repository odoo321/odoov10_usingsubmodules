<odoo>
        <template id="commissiondetails_report_template_id">
            <t t-call="report.html_container">
            <t t-foreach="docs" t-as="o">
             <t t-call ="property_management.property_external_layout_commission"> 
                       <div class="page">
                           <table style="border:solid 1px #000000"  width="100%" class="table table-bordered">
                                <tr>
                                     <td style="border: 1px solid #000000;">
                                          <b>From:</b>  
                                          <address t-field="o.property_id.company_id.partner_id"
                                                   t-options='{"widget": "contact", "fields": ["address", "name"], "no_marker": True}' />
                                     </td>
                                     <td style="border: 1px solid #000000;">
	                                     <b> To:</b>
	                                     <address t-field="o.company_id.partner_id"
                                                  t-options='{"widget": "contact", "fields": ["address", "name"], "no_marker": True}' />
                                     </td>
                                </tr>
                            </table>
                            <br/>
                            <table style="border:solid 1px #000000"  width="100%"  class="table table-condensed">
                                <tr>
                                   <th style="border: 1px solid #000000;height:25px" class="text-center">Seq.No:</th>
                                   <th style="border: 1px solid #000000;height:25px" class="text-center">Tenancy Name</th>
                                   <th style="border: 1px solid #000000;height:25px" class="text-center">Tenant Name</th>
                                   <th style="border: 1px solid #000000;height:25px" class="text-center">Property Name</th>
                                </tr>
                                <tr>
                                  <td style="border: 1px solid #000000;"  class="text-center">
                                       <span t-field = "o.number"/>
                                  </td>
                                  <td style="border: 1px solid #000000;" class="text-center"><span t-field = "o.patner_id.company_id.partner_id.name"/></td>
                                   <td style="border: 1px solid #000000;" class="text-center"><span t-field = "o.patner_id.name"/></td>
                                   <td style="border: 1px solid #000000;" class="text-center"><span t-field = "o.property_id.name"/></td>
                                 </tr>
                             </table>
                             <br/>
                            <table width="100%"  class="table table-condensed">
                             <thead>
                               <tr>
                                  <th style="border: 1px solid #000000;height:25px" class="text-center">Date</th>
                                 <th style="border: 1px solid #000000;height:25px" class="text-center">Status</th>
                                  <th style="border: 1px solid #000000;height:25px" class="text-center">Description</th>
                                  <th style="border: 1px solid #000000;height:25px" class="text-center">Commission Type</th>
                                  <th style="border: 1px solid #000000;height:25px" class="text-center">Rent Amount</th>
                                  <th style="border: 1px solid #000000;height:25px" class="text-center">Commission Amount</th>
                                </tr>
                             </thead>
                             <tbody class="invoice_tbody">    
                                <tr t-foreach="o.commission_line" t-as="a">        
                                  <td class="text-center"><span t-field = "a.date"/></td>
                                  <td class="text-center">
                                      <t t-if = "a.inv">
                                        <span>Paid</span>
                                      </t>
                                      <t t-if = "not a.inv">
                                        <span>Unpaid</span>
                                      </t>
                                  </td>
                                   <td  class="text-center"><span t-field = "a.name"/></td>
                                    <td class="text-center"><span t-field = "a.commission_ref"/></td>
                                     <td class="text-center"><span t-field = "a.rent_amt"/></td>
                                      <td class="text-center"><span t-field = "a.amount"/></td>
                                 </tr>
                             </tbody>
                         </table>
                         <br/>
                         <div class="row">
                           <div class="col-xs-4 pull-right">
                             <table width="100%"  class="table-collapsed">
                                 <tr class="border-black">
                                     <td colspan="5"  class="text-center"><b>Total:</b></td><td><b>:</b></td>
                                     <td class="text-left"><b><span t-esc = "o.amount_total"/></b></td>
                                 </tr>
                             </table>
                           </div>
                        </div>
                    </div>
                </t>
            </t> 
            </t>
       </template>
</odoo>
