<odoo>
    <template id="commission_invoice_owner">
        <t t-call="report.html_container">
            <t t-foreach="docs" t-as="o">
                <t t-call="property_commission.property_commission_external_layout">
                    <div class="page">
                        <div class="oe_structure" />
                        <div class="row">
                           <table class=" table " width="100%" style="border: 1px solid black;">
                                <tr>
                                    <th style="border-top: 1px solid;border-right:1px solid;" width="50%">FROM:</th>
                                    <th style="border-top: 1px solid; " width="50%">TO:</th>
                                </tr>

                                <td width="50%" style="border-top: 0px white;">
                                    
                                    <address t-field="o.company_id.parent_id.partner_id"
                                             t-options='{"widget": "contact", "fields": ["address", "name"], "no_marker": True}' />
                                </td>
                                <td width="50%" style="border-left: 1px solid; border-top: 0px white;">
                                    <address t-field="o.company_id.partner_id"
                                             t-options='{"widget": "contact", "fields": ["address", "name"], "no_marker": True}' />
                                </td>
                            </table>

                            <!-- <div class="row"> -->
                                <div class="text-center">
                                    <strong>From:</strong>
                                    <span t-esc="data['start_date']"/>&#160;&#160;&#160;
                                    <strong>To:</strong>
                                    <span t-esc="data['end_date']"/>
                                </div>
                            <!-- </div> -->
                            <table class="table-condensed">
                                <thead>
                                    <tr style="border: 1px solid black;">
                                        <th class="text-center" style="border: 1px solid black">Date</th>
                                        <th class="text-center" style="border: 1px solid black">Invoice#</th>
                                        <th class="text-center" style="border: 1px solid black">Description</th>
                                        <th class="text-center" style="border: 1px solid black">Source</th>
                                        <th class="text-center" style="border: 1px solid black">Property</th>
                                        <th class="text-center" style="border: 1px solid black">Tenancy</th>
                                        <th class="text-center" style="border: 1px solid black">Quantity</th>
                                        <th class="text-center" style="border: 1px solid black">Unit Price</th>
                                        <th class="text-center" style="border: 1px solid black">Taxes</th>
                                        <th class="text-center" style="border: 1px solid black">Amount</th>
                                    </tr>
                                  <!--   <tr>1</tr> -->
                                </thead>
                            <tbody class="invoice_tbody">
                                <tr t-foreach='get_data' t-as='l' style='page-break-inside: avoid;border-top: 1px solid black'>
                                    <td class='text-left'>
                                        <span t-esc="l['date']"/>
                                    </td>
                                    <td class='text-center'>
                                        <span t-esc="l['invoice_num']"/>
                                    </td>
                                    <td class='text-center'>
                                        <span t-esc="l['desc']"/>
                                    </td>
                                    <td class='text-left'>
                                        <span t-esc="l['source']"/>
                                    </td>
                                    <td class='text-center'>
                                        <span t-esc="l['property']"/>
                                    </td>
                                    <td class='text-left'>
                                        <span t-esc="l['tenancy']"/>
                                    </td>
                                    <td class='text-right'>
                                        <span t-esc="l['quantity']"/>
                                    </td>
                                    <td class='text-right'>
                                        <span t-esc="'%.3f'% l['unit_price']"/>
                                    </td>
                                    <td class='text-right'>
                                        <!-- <span t-esc="l['date']"/> -->
                                          <t t-if="l['tax']">
                                          <span t-esc="', '.join(map(lambda x: (x.description or x.name),l['tax']))"/></t>
                                          <t t-if="not l['tax']">
                                            <span class='text-center'> - </span>
                                        </t>
                                    </td>
                                    <td class='text-right'>
                                        <span t-esc="'%.3f'% l['amount']"/>
                                    </td>

                                </tr>
                            </tbody>

                            </table>
                                    <div class="row">
                <div class="col-xs-4 pull-right">
                    <table class="table table-condensed">
                        <tr class="border-black">
                            <td><strong>Subtotal</strong></td>
                            <td class="text-right">
                                <strong> <span t-esc="'%.3f'% get_sub_total"/> <span t-esc='o.company_id.parent_id.currency_id.symbol'/></strong>
                            </td>
                        </tr>
                            <tr t-if="get_tax">
                                 <td><strong>Tax</strong></td>
                                <td class="text-right"><strong><span t-esc="'%.3f'% get_tax"/> <span t-esc='o.company_id.parent_id.currency_id.symbol'/></strong></td>
                                <!-- <td class="text-right">
                                    <span t-esc="get_tax" t-options='{"widget": "monetary", "display_currency": o.currency_id}'/>
                                </td> -->
                            </tr>
                        <tr class="border-black">
                            <td><strong>Total</strong></td>
                            <td class="text-right">
                                <strong><span t-esc="'%.3f'% get_total"/> <span t-esc='o.company_id.parent_id.currency_id.symbol'/></strong>
                            </td>
                        </tr>
                    </table>
                </div>
            </div>
                    </div></div>
                </t>
            </t>
        </t>
    </template>
</odoo>
